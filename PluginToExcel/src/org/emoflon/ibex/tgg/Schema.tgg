#import "platform:/plugin/com.kaleidoscope.core.aux.simpletree/model/Simpletree.ecore"

#schema PluginsToExcel
	
#source {
	Simpletree
}

#target { 
	Simpleexcel
} 

#correspondence {
	SimpleTreeToSimpleExcelContainerRelation{
		#src->Node
		#trg->File
	}
	
	XMLFolderToExcelFileRelation{
		#src->Folder
		#trg->File
	}
	
	PluginFolderToExcelBlockHeaderRowRelation {
		#src->Simpletree.Folder
		#trg->Simpleexcel.Row
	}
	
	PluginNodeToExcelBlankRowRelation{
		#src->Node
		#trg->Row
	}
}

